{
  "name": "myanimelist-api-wrapper",
  "version": "1.1.2-b",
  "description": "A simple wrapper around the MAL API hosted on npm.",
  "main": "dist/bundle_umd.js",
  "scripts": {
    "build": "rollup -c",
    "build-umd": "rollup index.js --file dist/bundle_umd.js --format umd --name myanimelist-api-wrapper",
    "build-es": "rollup index.js --file dist/bundle_es.js --format es --name myanimelist-api-wrapper",
    "build-amd": "rollup index.js --file dist/bundle_amd.js --format amd --name myanimelist-api-wrapper",
    "build-iife": "rollup index.js --file dist/bundle_iife.js --format iife",
    "test": "mocha --timeout 10000 ./test/test-dist.js"
  },
  "repository": "https://github.com/prince-ao/my-anime-list",
  "author": "https://github.com/prince-ao",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.3",
    "rollup": "^2.60.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4"
  },
  "keywords": [
    "myanimelist",
    "MAL",
    "mal",
    "api-wrapper",
    "api",
    "anime",
    "manga"
  ]
}
